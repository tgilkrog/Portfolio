<Window x:Class="WpfDrinkzy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDrinkzy"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <Frame x:Name="Main" Margin="0,35,0,0" Background="Gray" NavigationUIVisibility="Hidden"/>
        <Button Content="Drink" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="130" Click="btnDrink" Height="35" Margin="10,0,0,526"/>
        <Button Content="Customer" HorizontalAlignment="Left" VerticalAlignment="Top" Width="130" Height="35" Margin="145,0,0,0" Click="Customer_click"/>
        <Button Content="User" HorizontalAlignment="Left" Margin="280,0,0,0" VerticalAlignment="Top" Width="130" Height="35" Click="User_Click"/>
        <Button Content="Ingredient" HorizontalAlignment="Left" Margin="415,0,0,0" VerticalAlignment="Top" Width="130" Height="35" Click="Ingredient_Click"/>
        <Button Content="Storage" HorizontalAlignment="Left" Margin="550,0,0,0" VerticalAlignment="Top" Width="130" Height="35" Click="Storage_click"/>
    </Grid>
</Window>
